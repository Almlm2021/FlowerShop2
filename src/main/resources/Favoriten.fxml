<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="529.0" prefWidth="810.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MiraMaro.com.GUI.FavoritenController">
   <children>
      <Label layoutX="300.0" layoutY="33.0" text="%ftitel">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TableView fx:id="tblFav" layoutX="86.0" layoutY="127.0" prefHeight="295.0" prefWidth="638.0">
        <columns>
          <TableColumn fx:id="tcID" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="tcName" prefWidth="158.0" text="%name" />
            <TableColumn fx:id="tcPrice" prefWidth="94.0" text="%preis" />
            <TableColumn fx:id="tcColor" prefWidth="112.0" text="%farbe" />
            <TableColumn fx:id="tcQuantity" prefWidth="84.0" text="%stueckzahl" />
            <TableColumn fx:id="tcType" prefWidth="114.0" text="Type" />
        </columns>
      </TableView>
      <Button layoutX="87.0" layoutY="452.0" mnemonicParsing="false" onAction="#backToMenu" prefHeight="40.0" prefWidth="156.0" text="%bmenu" />
      <Button layoutX="569.0" layoutY="452.0" mnemonicParsing="false" onAction="#removeFavourites" prefHeight="40.0" prefWidth="156.0" text="%delfav" />
   </children>
</AnchorPane>
